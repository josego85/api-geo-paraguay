{
  "name": "api-geo-paraguay",
  "description": "API GEO - Paraguay",
  "version": "2.16.0",
  "private": true,
  "main": "server.js",
  "author": {
    "name": "Josego",
    "email": "josego85@gmail.com"
  },
  "license": "GPlv3",
  "scripts": {
    "start": "nodemon --exec babel-node server.js",
    "dev": "webpack --mode development --progress",
    "build": "webpack --mode production --progress",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint . --no-cache",
    "lint:fix": "eslint --fix .",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest tests/integration",
    "test:unit": "jest tests/unit",
    "validate:api": "spectral lint openapi.yaml"
  },
  "bugs": {
    "url": "https://github.com/josego85/api-geo-paraguay/issues"
  },
  "homepage": "https://github.com/josego85/api-geo-paraguay/blob/master/README.md",
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.5.0",
    "expect-ct": "1.0.0",
    "express": "5.1.0",
    "express-locale": "2.0.2",
    "express-rate-limit": "7.5.0",
    "feature-policy": "0.6.0",
    "geoip-lite": "1.4.10",
    "graphql": "16.11.0",
    "graphql-yoga": "5.13.4",
    "helmet": "8.1.0",
    "ioredis": "^5.6.1",
    "js-yaml": "4.1.0",
    "mongoose": "8.14.1",
    "ms": "2.1.3",
    "mysql2": "3.14.1",
    "newrelic": "12.8.2",
    "ngeohash": "0.6.3",
    "node-polyglot": "2.6.0",
    "send": "1.2.0",
    "swagger-ui-express": "5.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.27.1",
    "@babel/eslint-parser": "7.27.1",
    "@babel/node": "7.27.1",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-runtime": "7.27.1",
    "@babel/preset-env": "7.27.1",
    "@stoplight/spectral-cli": "6.11.1",
    "babel-loader": "10.0.0",
    "babel-plugin-module-resolver": "5.0.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "dotenv-webpack": "8.1.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-node": "4.1.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-import-resolver-babel-module": "5.3.2",
    "eslint-import-resolver-webpack": "0.13.10",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.2.6",
    "jest": "29.7.0",
    "nodemon": "3.1.10",
    "prettier": "3.5.3",
    "supertest": "7.1.0",
    "swagger-jsdoc": "6.2.8",
    "webpack": "5.99.7",
    "webpack-cli": "6.0.1",
    "webpack-node-externals": "3.0.0"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 22"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup/jest.setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/config/**",
      "!src/docs/**"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ]
  }
}
